#include <iostream>
#include "Image_Class.h"
using namespace std;

int main()
{
    Image img("luffy.jpg");

    for (int i = 0; i < img.width; i++)
    {
        for (int j = 0; j < img.height; j++) 
        {
            for (int k = 0; k < 3; k++)
            {
                img(i, j, k) = 255 - img(i, j, k);
            }
        }
    }
    img.saveImage("luffyy.jpg");
    cout << "Hello" << endl;
    return 0;
}
